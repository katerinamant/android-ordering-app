<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/background">

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="28dp"
        android:text="@string/Address"
        android:textSize="20sp"
        app:layout_constraintTop_toBottomOf="@+id/txt_manager_info_header"
        tools:layout_editor_absoluteX="31dp" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="40dp"
        android:text="@string/Phone_Number"
        android:textSize="20sp"
        app:layout_constraintTop_toBottomOf="@+id/edit_txt_mngr_address_infoinput"
        tools:layout_editor_absoluteX="31dp" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="40dp"
        android:text="@string/SSN"
        android:textSize="20sp"
        app:layout_constraintTop_toBottomOf="@+id/edit_txt_mngr_phoneNumber_infoinput"
        tools:layout_editor_absoluteX="31dp" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="40dp"
        android:text="@string/Brand"
        android:textSize="20sp"
        app:layout_constraintTop_toBottomOf="@+id/edit_txt_mngr_ssn_infoinput"
        tools:layout_editor_absoluteX="31dp" />

    <TextView
        android:id="@+id/txt_manager_info_header"
        android:layout_width="269dp"
        android:layout_height="92dp"
        android:layout_marginTop="56dp"
        android:fontFamily="sans-serif-medium"
        android:gravity="center"
        android:text="@string/Info"
        android:textAppearance="@style/TextAppearance.AppCompat.Large"
        android:textColor="@color/text"
        android:textSize="32sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/edit_txt_mngr_address_infoinput"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:ems="10"
        android:inputType="text"
        app:layout_constraintTop_toBottomOf="@+id/textView4"
        tools:layout_editor_absoluteX="31dp" />

    <EditText
        android:id="@+id/edit_txt_mngr_phoneNumber_infoinput"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:ems="10"
        android:inputType="text"
        app:layout_constraintTop_toBottomOf="@+id/textView6"
        tools:layout_editor_absoluteX="31dp" />

    <EditText
        android:id="@+id/edit_txt_mngr_ssn_infoinput"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:ems="10"
        android:inputType="text"
        app:layout_constraintTop_toBottomOf="@+id/textView7"
        tools:layout_editor_absoluteX="31dp" />

    <EditText
        android:id="@+id/edit_txt_mngr_brand_infoinput"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:ems="10"
        android:inputType="text"
        app:layout_constraintTop_toBottomOf="@+id/textView8"
        tools:layout_editor_absoluteX="29dp" />

    <Button
        android:id="@+id/edit_txt_mngr_finish_infoinput"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:layout_marginEnd="100dp"
        android:backgroundTint="@color/dark_button"
        android:text="Finish"
        android:textSize="16dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/edit_txt_mngr_brand_infoinput" />
</androidx.constraintlayout.widget.ConstraintLayout>